    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "bun-types": ">=1.0.0",
    "marked": "^17.0.1",
    "oxlint": ">=0.5.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": ">=5.0.0",
    "vite": "^5.0.0"
